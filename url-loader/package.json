{
  "name": "url-loader",
  "version": "1.0.0",
  "description": "Downloads files from APIs and stores them in Azure storage",
  "main": "index.js",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "axios": "^0.26.0",
    "azure-storage": "^2.10.1",
    "bottleneck": "^2.19.5",
    "copyfiles": "^2.4.1",
    "dotenv": "^10.0.0",
    "joi": "^13.6.0",
    "js-yaml": "^4.1.0",
    "knex": "^1.0.4",
    "knex-postgis": "^0.14.2",
    "mongoose": "^6.0.13",
    "pg": "^8.7.3",
    "typescript": "^4.6.4",
    "underscore": "^1.12.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@types/jest": "^26.0.20",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-watch": "^7.0.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^3.0.0"
  },
  "resolutions": {},
  "husky": {
    "hooks": {
      "pre-push": "npm run lint:ci"
    }
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "watch": "tsc -w",
    "start": "node dist/index.js",
    "copyymls": "copyfiles --error --verbose  sources/*.*  /app/dist",
    "postbuild": "npm run copyymls --scripts-prepend-node-path",
    "lint": "eslint . --fix --max-warnings=0",
    "lint:ci": "eslint . --max-warnings=0",
    "prepush": "eslint . --max-warnings=0",
    "test": "NODE_ENV=test jest --forceExit --verbose --colors --testTimeout=10000 --detectOpenHandles",
    "test:coverage": "yarn test -- --coverage",
    "test:watch": "yarn test -- --watchAll"
  },
  "author": "",
  "license": "MIT"
}
